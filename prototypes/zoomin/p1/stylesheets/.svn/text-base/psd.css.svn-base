.w {
  background-color: rgba( 255, 255, 255,.9);
  box-shadow: 0px 1px 3px 0px rgb( 0, 0, 0 );
  width: 1000px;
  margin: 0 auto;
  z-index: 101;
}

.settings-nav {
  border-radius: 4px;
  background-image: -moz-linear-gradient( 90deg, rgb( 132, 132, 132 ) 0%, rgb( 238, 238, 238 ) 16.553%, rgb( 255, 255, 255 ) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb( 132, 132, 132 ) 0%, rgb( 238, 238, 238 ) 16.553%, rgb( 255, 255, 255 ) 100%);
  box-shadow: 0px 1px 3px 0px rgb( 0, 0, 0 );
  width: 275px;
  height: 40px;
  z-index: 82;
}
